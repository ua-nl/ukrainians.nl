{
  "name": "ukrainians.nl",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "fmt": "npm run fmt:code && npm run fmt:md",
    "fmt:code": "turbo run lint -- --fix",
    "fmt:md": "prettier --write '**/*.{md}'"
  },
  "devDependencies": {
    "turbo": "1.2.16"
  }
}
